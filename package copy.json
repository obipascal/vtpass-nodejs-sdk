{
    "name": "@obipascal/paystack-sdk",
    "version": "1.0.9",
    "main": "./lib/cjs/index.js",
    "module": "./lib/es/index.js",
    "types": "./lib/es/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/es/index.js",
            "require": "./lib/cjs/index.js"
        }
    },
    "description": "A comprehensive paystack API sdk for nodejs written in typescript",
    "scripts": {
        "test": "jest",
        "build:es": "tsc -p tsconfig.es.json",
        "build:test": "tsc -p tsconfig.test.json",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build": "npm run build:es && npm run build:cjs"
    },
    "keywords": [
        "payment",
        "API",
        "naira",
        "paystack"
    ],
    "author": "Obi Pascal <pascalobi83@gmail.com> (https://obi-works.vercel.app)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/obipascal/paystack-nodejs-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/obipascal/paystack-nodejs-sdk/issues"
    },
    "homepage": "https://github.com/obipascal/paystack-nodejs-sdk",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.4"
    },
    "dependencies": {
        "axios": "^1.6.8"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    }
}
